    "keywords":  [
        "Tbx",
        "tools",
        "tropotek"
    ],
    "time": "2018-12-20",
    "license": "MIT",
    "version": "1.3.2",
    "authors":  [
         {
            "name": "Michael Mifsud",
            "email": "info@tropotek.com.au",
            "homepage": "http://www.tropotek.com/"
        }
